<% dishes.forEach((element) => { %>
<div class="col col-lg-2">
  <div
    class="menu-poster-1 d-flex flex-column justify-content-between align-items-center"
  >
    <h5 class="h4 text-white"><%= element.name %></h5>
    <img src="<%= element.thumbnail %>" alt="" />
    <span><%= element.price %></span>
  </div>
</div>
<% }) %>
